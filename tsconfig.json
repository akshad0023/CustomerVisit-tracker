{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": ["node"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/react-native-action-sheet.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
